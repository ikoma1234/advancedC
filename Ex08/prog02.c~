#include <stdio.h>
#include <math.h>

typedef struct {
  double  x; /* x座標 */
  double  y; /* y座標 */
} XY; /* 平面上の点 */

typedef struct {
  XY center;
  XY p;
} CIRCLE; /* centerを中心点、pを円周上の点とする円 */

CIRCLE input1(void);   /* データを読み込んだ構造体を戻す */
void input2(CIRCLE *);/* 構造体のポインタを渡し、そこにデータを読み込む */

int main(){

  struct XY xy;
  struct CIRCLE circle;

  printf("データを入力して下さい（構造体を返す関数）：");
  scanf("%d%d%d%d", &xy.x,&xy.y,&circle.center,&circle.p);

  printf("input1 : r, length, area : %f, %f, %f",);

  printf("データを入力して下さい（構造体ポインタを使う関数）：");
  scanf("%d%d%d%d", &xy.x,&xy.y,&circle.center,&circle.p);

  printf("input1 : r, length, area : %f, %f, %f",);

  return 0;
}

CIRCLE input1(){

}

void input2(CIRCLE *){

}

  
