#include <stdio.h>
#define func(x) x*x*x - 2*x*x + 3*x + 4

int main()
{
    int a = 8;
    double b = 5.5;

    printf("a=%d, b=%f\n", a, b);
    printf("func(a)=%d\n", func(a));
    printf("func(a+3)=%d\n", func(a + 3));
    printf("func(a)*2=%d\n", func(a) * 2);
    printf("func(b)=%f\n", func(b));
    printf("func(b+3.5)=%f\n", func(b + 3.5));

    return 0;
}
