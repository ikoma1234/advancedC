#include <stdio.h>
int main()
{
   int i;
   int array[] = {11,22,33,44};
   long l_array[] = {123,234,345,456};
   char c_array[] = {'a','b','c','d'};
   char str_array[] = {"A","BC","DEF","GHIJK"};
   double d_array[] = {1e-3, 2e-4, 3e-5, 4e-6};
   char array_2dc[][10] = {"a","bcde","fgh","ij"};


   for(i = 0 ; i < 4 ; i++){
     printf( "%d %p %d\n",i,&array[i],array[i]);
     printf( "%d %p %d\n",i,&l_array[i],l_array[i]);
     printf( "%d %p %d\n",i,&c_array[i],c_array[i]);
     printf( "%d %p %d\n",i,&str_array[i],str_array[i]);
     printf( "%d %p %d\n",i,&d_array[i],d_array[i]);
     printf( "%d %p %d\n",i,array_2dc[i],array_2dc[i]);
   }
     
   printf("\n%p\n",array);
   printf("\n%p\n",l_array);
   printf("\n%p\n",c_array);
   printf("\n%p\n",str_array);
   printf("\n%p\n",d_array);
   printf("\n%p\n",array_2dc);

   return 0;
}
